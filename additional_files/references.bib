	issn = {1549-5469},
	journal = {Genome Research},
	number = {4},
	pages = {581--591},
	pmid = {29535149},
	title = {{SvABA: genome-wide detection of structural variants and indels by local assembly.}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/29535149%0Ahttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5880247},
