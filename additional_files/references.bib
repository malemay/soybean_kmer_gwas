	file = {:home/archandre/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Campbell et al. - 2016 - Fast neutron-induced structural rearrangements at a soybean NAP1 locus result in gnarled trichomes.pdf:pdf},
	issn = {0040-5752},
	journal = {Theoretical and Applied Genetics},
	month = {sep},
	number = {9},
	pages = {1725--1738},
	pmid = {27282876},
	publisher = {Springer Berlin Heidelberg},
	title = {{Fast neutron-induced structural rearrangements at a soybean NAP1 locus result in gnarled trichomes}},
	url = {http://link.springer.com/10.1007/s00122-016-2735-x},
