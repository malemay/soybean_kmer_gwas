	journal = {Genetics},
	month = {oct},
	number = {2},
	pages = {995--1007},
	title = {Genetic Redundancy in Soybean Photoresponses Associated With Duplication of the Phytochrome {A} Gene},
	url = {https://academic.oup.com/genetics/article/180/2/995/6073722},
	volume = {180},
