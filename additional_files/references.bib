	file = {:home/archandre/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Dom{\'{i}}nguez et al. - 2020 - The impact of transposable elements on tomato diversity.pdf:pdf},
	issn = {2041-1723},
	journal = {Nature Communications},
	month = {dec},
	number = {1},
	pages = {4058},
	publisher = {Springer US},
	title = {{The impact of transposable elements on tomato diversity}},
	url = {http://dx.doi.org/10.1038/s41467-020-17874-2 https://www.nature.com/articles/s41467-020-17874-2},
